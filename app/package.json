    "hubot-diagnostics": "0.0.1",
    "hubot-help": "^0.1.3",
    "hubot-redis-brain": "0.0.3",
    "hubot-slack": "^3.4.2"
  },
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "babel-eslint": "^6.0.3",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "chai": "^3.5.0",
